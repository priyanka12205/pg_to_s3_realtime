# etl/.env.example
# Copy to .env and fill values (quotes optional).

# PostgreSQL
PG_HOST=localhost
PG_PORT=5432
PG_DB=postgres
PG_USER=postgres
PG_PASSWORD=postgres

# AWS (assumes you've already configured AWS CLI or instance role)
AWS_REGION=us-east-1
S3_BUCKET=your-bucket-name
S3_PREFIX=raw

# Table to read and which timestamp column to use as watermark
TABLE_NAME=public.orders
WATERMARK_COLUMN=updated_at

# Optional: FX rate to convert USD->INR for demo transformation
USD_TO_INR=83.0